pragma solidity ^0.8;

contract Calci{
    function Addition(uint number1,uint number2)public pure returns(uint){
        return number1+number2;
    }

    function Subtraction(uint number1,uint number2)public pure returns(uint){
        return number1+number2;
    }

    function Multiplication(uint number1,uint number2)public pure returns(uint){
        return number1+number2;
    }

    function Division(uint number1,uint number2)public pure returns(uint){
        return number1+number2;
    }
}